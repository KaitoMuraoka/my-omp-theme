{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "upgrade": {
    "source": "cdn",
    "interval": "168h",
    "auto": false,
    "notice": false
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#0089ae",
          "foreground": "#fff",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "windows": "\uf179"
          },
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "background": "#feae34",
          "foreground": "#262b44",
          "powerline_symbol": "\ue0b0",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "üìÅ {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            },
            "source": "cli",
            "mapped_branches": {
              "main": "üè†",
              "feat/*": "üöÄ ",
              "feature/*": "üöÄ ",
              "bug/*": "üêõ "
            }
          }
        },
        {
          "background": "#0095e9",
          "background_templates": [
            "{{ if gt .Code 0 }}#ff0044{{ end }}"
          ],
          "foreground": "#ffffff",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " \uf0e7 ",
          "trailing_diamond": "\ue0b4",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#f36943",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#33DD2D{{end}}",
            "{{if eq \"Discharging\" .State.String}}#FFCD58{{end}}",
            "{{if eq \"Full\" .State.String}}#0476d0{{end}}"
          ],
          "foreground": "#242424",
          "invert_powerline": true,
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "<transparent,background>\ue0b2",
          "style": "diamond",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 \ueb2d ",
          "type": "battery"
        },
        {
          "background": "#0087D8",
          "foreground": "#003544",
          "invert_powerline": true,
          "properties": {
            "display_mode": "context",
            "fetch_virtual_env": true
          },
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue235 {{ .Venv }} ",
          "type": "python"
        },
        {
          "background": "#4ead29",
          "foreground": "#fff",
          "invert_powerline": true,
          "trailing_diamond": "\ue0b4",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "<#fff> \ue641 </>{{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "version": 3,
  "final_space": true
}
